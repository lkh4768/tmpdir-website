{
  "name": "website",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:prd": "NODE_ENV=production node build/app.js",
    "start:dev": "NODE_ENV=development node build/app.js",
    "clean": "rm -rf build",
    "build:dev": "webpack --config webpack.config.dev.js --progress --profile --colors && NODE_ENV=development webpack --config webpack.config.server.js --progress --profile --colors",
    "build:prd": "webpack --config webpack.config.client.js --progress --profile --colors && webpack --config webpack.config.server.js --progress --profile --colors",
    "all:prd": "npm run clean && npm run build:prd && npm run start:prd",
    "all:dev": "npm run clean && npm run build:dev && npm run start:dev",
    "test": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:watch": "npm run test -- --watch",
    "analyzer:prd": "npm run clean && npm run build:prd && rm -rf stats.json && webpack --config webpack.config.client.js --profile --json > stats.json && webpack-bundle-analyzer stats.json build -h 0.0.0.0"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "NODE_ENV": "test"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/script/staticTransformer.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupFiles": [
      "<rootDir>/src/tests/global.js"
    ],
    "modulePathIgnorePatterns": [
      "app.js",
      "global.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/server/config",
      "<rootDir>/src/server/routes",
      "<rootDir>/src/tests"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/server/config",
      "<rootDir>/src/server/routes",
      "<rootDir>/src/tests"
    ],
    "moduleNameMapper": {
      "^_tests(.*)$": "<rootDir>/src/tests/$1",
      "^_components/(.*)$": "<rootDir>/src/public/components/$1",
      "^_containers/(.*)$": "<rootDir>/src/public/containers/$1",
      "^_common/(.*)$": "<rootDir>/src/public/common/$1",
      "^_static/(.*)$": "<rootDir>/src/public/static/$1",
      "^_entities/(.*)$": "<rootDir>/src/public/entities/$1",
      "^_data/(.*)$": "<rootDir>/src/public/data/$1",
      "^_app/(.*)$": "<rootDir>/src/public/app/$1",
      "^_pages/(.*)$": "<rootDir>/src/public/pages/$1",
      "^_modules/(.*)$": "<rootDir>/src/server/modules/$1",
      "^_routes/(.*)$": "<rootDir>/src/server/routes/$1",
      "^_config/(.*)$": "<rootDir>/src/server/config/$1",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "coverageDirectory": "./reports/coverage",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testResultsProcessor": "jest-junit"
  },
  "jest-junit": {
    "output": "./reports/junit.xml"
  },
  "repository": {
    "type": "git",
    "url": "https://dev.sw-warehouse.xyz:1744/tmp-dir/Website.git"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.11",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.0.0",
    "jest-junit": "^5.1.0",
    "node-sass": "^4.9.2",
    "postcss-loader": "^2.1.0",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-popper": "^0.8.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "webpack": "^4.6.0",
    "webpack-assets-manifest": "^3.0.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0",
    "bunyan": "^1.8.12",
    "bunyan-rotating-file-stream": "^1.6.3",
    "connect-multiparty": "^2.1.1",
    "express": "^4.16.2",
    "express-bunyan-logger": "^1.3.3",
    "file-loader": "^1.1.6",
    "form-data": "^2.3.2",
    "react": "^16.3.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-share": "^2.0.0",
    "reactstrap": "^5.0.0-beta.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
